  "description": "Production-ready multi-agent orchestration with database-mediated coordination",
  "main": "index.js",
  "keywords": [
    "ai-agents",
    "multi-agent",
    "orchestration", 
    "database-coordination",
    "aws",
    "enterprise-automation",
    "claude-code"
  ],
  "author": "HappyHippo.ai",
  "license": "MIT",
  "homepage": "https://github.com/happyhippo-ai/equilateral-agents-open-core",
  "repository": {
    "type": "git",
    "url": "https://github.com/happyhippo-ai/equilateral-agents-open-core.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
