    "setup": "node scripts/setup.js",
    "agents": "node equilateral-core/cli/orchestrator-cli.js",
    "migrate": "node scripts/migrate.js",
    "health": "node scripts/health-check.js"
  },
  "dependencies": {
    "sqlite3": "^5.1.6",
    "sqlite": "^5.1.1",
    "pg": "^8.11.3",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.3"
  },
  "peerDependencies": {
    "pg": "^8.0.0"
  },
  "config": {
    "database": {
      "default": "sqlite",
      "sqlite": {
        "file": "./data/equilateral-agents.db",
